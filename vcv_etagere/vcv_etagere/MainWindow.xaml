<Window x:Class="vcv_etagere.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:vcv_etagere"
        Title="Rack"
        Height="500"
        Width="900">

    <Grid Background="#222">

        <!-- Layer modules -->
        <StackPanel Orientation="Horizontal"
                Panel.ZIndex="0">

            <local:VcoModule x:Name="MyVcoModule" Margin="50"/>
            <local:AudioOutModule x:Name="MasterModule" Margin="50"/>

        </StackPanel>

        <!-- Layer câbles -->
        <Canvas x:Name="CableLayer"
        Panel.ZIndex="1"
        IsHitTestVisible="False"/>
         

    </Grid>



</Window>
